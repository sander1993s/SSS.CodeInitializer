<Window x:Class="CodeInitializer.DialogBoxes.IncludeGenerics.GenericOptionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Generic Option" Height="150" Width="300">
    <StackPanel Margin="10">
        <TextBlock Text="Include generics in interface?" Margin="0,0,0,10"/>
        <CheckBox x:Name="IncludeGenericsCheckBox" Content="Include generics" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,10,0,0">
            <Button Content="OK" Width="75" Margin="5,0,0,0" Click="Ok_Click"/>
            <Button Content="Cancel" Width="75" Margin="5,0,0,0" Click="Cancel_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
